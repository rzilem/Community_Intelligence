+      messages: [
+        {
+          role: 'system',
+          content: PromptTemplates.SYSTEM_CONTEXT.replace('{context}', JSON.stringify(this.context))
+        },
+        { role: 'user', content: prompt }
+      ],
+      response_format: { type: 'json_object' },
