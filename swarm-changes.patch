+-- =====================================================
+-- ROW LEVEL SECURITY (RLS)
+-- =====================================================
+
+-- Enable RLS on all tables
+ALTER TABLE portfolios ENABLE ROW LEVEL SECURITY;
+ALTER TABLE hoas ENABLE ROW LEVEL SECURITY;
+ALTER TABLE user_hoa_access ENABLE ROW LEVEL SECURITY;
