+        .select('*')
+        .eq('id', 'hoa-2')
+        .single();
+        
+      expect(error2).toBeDefined();
+      expect(hoa2).toBeNull();
+    });
+
+    test('Cascade deletes work correctly', async () => {
+      // Create HOA with properties
+      const { data: hoa } = await supabase
+        .from('hoas')
